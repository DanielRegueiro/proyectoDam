package com.proyecto.controlador;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

import com.proyecto.service.TropasService;

@RestController
public class TropasController {
    
    private final TropasService tropasService;

    public TropasController(TropasService tropasService) {
        this.tropasService = tropasService;
    }
    
    @GetMapping("/gettropas")
    public String getTropas() {
        return "add-user";
    }
   
}
