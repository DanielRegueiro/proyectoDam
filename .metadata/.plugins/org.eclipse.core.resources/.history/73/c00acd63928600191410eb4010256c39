    @FXML
    private ImageView hielo;

    @FXML
    private Button botonTodas;

    @FXML
    private Button botonSeleccionar;

    @FXML
    private ListView<?> listaTropasPoseidas;

    @FXML
    void Volver(ActionEvent event) {

    }

    @FXML
    void abrirGeneradorListas(ActionEvent event) {

    }

    @FXML
