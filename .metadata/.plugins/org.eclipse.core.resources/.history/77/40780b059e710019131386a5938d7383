package com.proyecto.controlador;

import java.net.URL;

import javafx.event.ActionEvent;
import javafx.fxml.FXML;
import javafx.scene.control.Button;
import javafx.scene.image.Image;
import javafx.scene.image.ImageView;

public class LogosController {

    @FXML
    private Button botonLobos;
    
    @FXML
    private void botonLobos_click(ActionEvent event) {
    	
    	
    }
    
 
    
    public LogosController() {
		super();
		colocarImagenesBotones();
	}


	private void colocarImagenesBotones() {
    	URL linkLobos = getClass().getResource("/img/lobos.png");
    	
    	Image imagenLobo= new Image(linkLobos.toString(),100,150,false,true);
    	
    	botonLobos.setGraphic(new ImageView(imagenLobo));
    	
    }
 
}